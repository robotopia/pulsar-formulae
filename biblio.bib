@article{Luo1992,
abstract = {Maser curvature emission in the presence of curvature drift is$\backslash$nre-examined in detail. A physical interpretation of this mechanism is$\backslash$ngiven. The maser emission is possible only when there is a beam-type$\backslash$ndistribution of outflowing electrons or positrons with Lorentz factors$\backslash$nsatisfying gamma of greater than or approximately equal to 1000,$\backslash$nwhich is determined by the geometry of the pulsar magnetosphere.$\backslash$nIt is shown that, for amplification to be effective in the pulsar$\backslash$nmagnetosphere, the density of the electrons or positrons in the beam must be$\backslash$nclose to a critical value beyond which the particle flow is no longer$\backslash$ncontrolled by the magnetic field. It is also shown that, to obtain$\backslash$nappreciable amplification in the range 2 to 200 MHz, the magnetic$\backslash$nflux density in the source region must be B greater than 10 exp 8$\backslash$nG. This seems to rule out the mechanism for millisecond pulsars.},
