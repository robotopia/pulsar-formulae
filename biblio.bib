@article{Bhattacharyya2007,
abstract = {We report the discovery of a remarkable subpulse drift pattern in the  relatively less-studied wide profile pulsar B0818-41 using  high-sensitivity Giant Metrewave Radio Telescope (GMRT) observations. We  find simultaneous occurrences of three drift regions with two different  drift rates: an inner region with steeper apparent drift rate flanked on  each side by a region of slower apparent drift rate. Furthermore, these  closely spaced drift bands always maintain a constant phase  relationship. Though these drift regions have significantly different  values for the measured P{\textless}SUB{\textgreater}2{\textless}/SUB{\textgreater}, the measured P{\textless}SUB{\textgreater}3{\textless}/SUB{\textgreater} value  is the same and equal to 18.3P{\textless}SUB{\textgreater}1{\textless}/SUB{\textgreater}. We interpret the unique  drift pattern of this pulsar as being created by the intersection of our  line of sight (LOS) with two conal rings on the polar cap of a fairly  aligned rotator (inclination angle alpha {\~{}} 11∞), with an `inner'  LOS geometry (impact angle ). We argue that both rings have the same  values for the carousel rotation periodicity P{\textless}SUB{\textgreater}4{\textless}/SUB{\textgreater} and the  number of sparks N{\textless}SUB{\textgreater}sp{\textless}/SUB{\textgreater}. We find that N{\textless}SUB{\textgreater}sp{\textless}/SUB{\textgreater} is 19-21  and show that it is very likely that P{\textless}SUB{\textgreater}4{\textless}/SUB{\textgreater} is the same as the  measured P{\textless}SUB{\textgreater}3{\textless}/SUB{\textgreater}, making it a truly unique pulsar. We present  results from simulations of the radiation pattern using the inferred  parameters, which support our interpretations and reproduce the average  profile as well as the observed features in the drift pattern quite  well.},
archivePrefix = {arXiv},
arxivId = {astro-ph/0702402},
author = {Bhattacharyya, B. and Gupta, Y. and Gil, J. and Sendyk, M.},
doi = {10.1111/j.1745-3933.2007.00293.x},
eprint = {0702402},
file = {:home/smcsweeney/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Bhattacharyya et al. - 2007 - Discovery of a remarkable subpulse drift pattern in PSR B0818-41.pdf:pdf},
issn = {17453933},
