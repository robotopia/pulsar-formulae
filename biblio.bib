issn = {0004-6361},
journal = {Astronomy and Astrophysics},
keywords = {B0031-07,J0034-0721,stars: neutron,stars: pulsars: general,stars: pulsars: individual: PSR B0031-07},
mendeley-tags = {B0031-07,J0034-0721},
number = {2},
pages = {575--579},
title = {{Giant pulses in pulsar PSR B0031-07}},
url = {http://adsabs.harvard.edu/abs/2004A{\&}A...427..575K},
