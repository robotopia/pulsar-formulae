issn = {00046264},
journal = {Publications of the Astronomical Society of Japan},
keywords = {magnetic fields,plasmas,pulsars: general},
number = {3},
pages = {43--51},
title = {{A Particle Simulation for the Pulsar Magnetosphere: Relationship of Polar Cap, Slot Gap, and Outer Gap}},
url = {http://adsabs.harvard.edu/abs/2012PASJ...64...43Y},
volume = {64},
