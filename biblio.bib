@article{Yang2014,
abstract = {Pulsar nulling is a phenomenon of sudden cessation of pulse emission for a number of periods. The nulling fraction was often used to characterize the phenomenon. We propose a new method to analyse pulsar nulling phenomenon, by involving two key parameters, the nulling degree, {\$}\backslashchi{\$}, which is defined as the angle in a rectangular coordinates for the numbers of nulling periods and bursting periods, and the nulling scale, {\$} N {\$}, which is defined as the effective length of the consecutive nulling periods and bursting periods. The nulling degree {\$}\backslashchi{\$} can be calculated by {\$}\backslashtan \backslashchi = N{\_}{\{}\backslashrm nulling{\}} / N{\_}{\{}\backslashrm bursting{\}} {\$} and the mean is related to the nulling fraction, while the nulling scale, {\$} N {\$}, is also a newly defined fundamental parameter which indicates how often the nulling occurs. We determined the distributions of {\$}\backslashchi{\$} and {\$} N {\$} for 10 pulsars by using the data in literature. We found that the nulling degree {\$}\backslashchi{\$} indicates the relative length of nulling to that of bursting, and the nulling scale {\$} N {\$} is found to be related to the derivative of rotation frequency and hence the loss rate of rotational energy of pulsars. Their deviations reflect the randomness of the nulling process.},
archivePrefix = {arXiv},
arxivId = {1310.6610},
author = {Yang, Aiyuan and Han, Jinlin and Wang, Na},
doi = {10.1007/s11433-014-5534-x},
eprint = {1310.6610},
file = {:home/smcsweeney/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Yang, Han, Wang - 2014 - A new method to analyse pulsar nulling phenomenon.pdf:pdf},
issn = {16747348},
journal = {Science China: Physics, Mechanics and Astronomy},
keywords = {pulsars,radiation mechanisms,radio sources},
number = {8},
pages = {1600--1606},
title = {{A new method to analyse pulsar nulling phenomenon}},
volume = {57},
year = {2014}
}
