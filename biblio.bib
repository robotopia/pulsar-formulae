file = {:home/smcsweeney/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Heger et al. - 2003 - How Massive Single Stars End their Life.pdf:pdf},
issn = {0004-637X, 1538-4357},
journal = {The Astrophysical Journal},
keywords = {Astrophysics},
mendeley-tags = {Astrophysics},
