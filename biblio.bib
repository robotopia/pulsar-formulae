issn = {00358711},
journal = {Monthly Notices of the Royal Astronomical Society},
keywords = {B1918+19,General-pulsars,Individual,Non-thermal-pulsars,Polarisation,Polarization-Radiationmechanisms},
mendeley-tags = {Polarisation},
number = {1},
pages = {445--456},
title = {{Drifting, moding and nulling: Another look at pulsar B1918+19}},
