doi = {10.1088/0067-0049/204/1/12},
eprint = {1210.2573},
file = {:home/smcsweeney/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Maan et al. - 2013 - RRI-GBT Multi-band Receiver Motivation, Design, and Development.pdf:pdf},
isbn = {0067-0049},
