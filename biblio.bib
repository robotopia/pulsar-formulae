file = {:home/smcsweeney/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Bhattacharyya, Gupta, Gil - 2009 - Exploring the remarkable subpulse drift and polarization properties of PSR B0818-41.pdf:pdf},
