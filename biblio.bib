keywords = {Gamma,Polarisation,Polarization,morpho,profile},
mendeley-tags = {Polarisation},
number = {2003},
pages = {49},
primaryClass = {astro-ph},
title = {{Relativistic effects and polarization in three high-energy pulsar models}},
