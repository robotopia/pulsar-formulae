issn = {13652966},
journal = {Monthly Notices of the Royal Astronomical Society},
keywords = {B0031-07,J0034-0721,general,non-thermal,pulsar,radiation mechanisms},
mendeley-tags = {B0031-07,J0034-0721},
number = {1},
