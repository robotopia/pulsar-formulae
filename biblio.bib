@article{Lange1998,
abstract = {We report on observations of microstructure at 4.85 and 1.41$\backslash$nGHz of pulsars strong enough for the observation of single pulses$\backslash$nwith time resolution between 7 mu s and 160 mu s. All five pulsars$\backslash$nobserved at 4.85 GHz show microstructure in a large fraction of their$\backslash$npulses. We confirm that microstructure seems to be a common property of$\backslash$nthe pulsar emission mechanism and not only an additional feature$\backslash$nof some pulsars. We show that, for those pulsars observed earlier$\backslash$nat lower frequencies, there is little variation in microstructure$\backslash$nparameters at different frequencies. The fraction of pulses showing$\backslash$nmicrostructure tends to be higher at 4.85 GHz when compared with$\backslash$nlower frequency observations. We argue that the short time-scale of$\backslash$nmicrostructure reflects the extension of the emitting structures.},
