issn = {2251-1717},
journal = {Journal of Astronomical Instrumentation},
keywords = {direction-dependent gridding,epoch of reionization,polarization,snapshot imaging,wide-field synthesis imaging},
number = {01},
pages = {1250003},
title = {{Direction-Dependent Polarized Primary Beams in Wide-Field Synthesis Imaging}},
