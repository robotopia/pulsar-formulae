author = {Patrikalakis, Nicholas M. and Maekawa, Takashi},
booktitle = {Shape Interrogation for Computer Aided Design and Manufacturing},
doi = {10.1007/978-3-642-04074-0},
eprint = {arXiv:1011.1669v3},
file = {:home/smcsweeney/library/shape{\_}interrogation{\_}for{\_}computer{\_}aided{\_}design{\_}and{\_}manufacturing.pdf:pdf},
