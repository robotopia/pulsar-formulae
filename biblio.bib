journal = {Astronomy and Astrophysics},
keywords = {B0031-07,J0034-0721,PSR B0031-07,Polarisation,drift,geometry,morpho,orthog mode,polarization,rfm,subpulse},
mendeley-tags = {B0031-07,J0034-0721,Polarisation},
number = {1626},
pages = {10},
