title = {{Precision Timing of PSR J0437−4715: An Accurate Pulsar Distance, a High Pulsar Mass, and a Limit on the Variation of Newton's Gravitational Constant}},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data{\_}query?bibcode=2008ApJ...679..675V{\&}link{\_}type=ABSTRACT{\%}5Cnpapers2://publication/doi/10.1086/529576{\%}5Cnhttp://stacks.iop.org/0004-637X/679/i=1/a=675},
