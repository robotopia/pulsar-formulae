issn = {0004-6361},
journal = {Astronomy and Astrophysics},
keywords = {B0320+39,B0809+74,PSR B0320+39,PSR B0809+74,drift,morpho,subpulse},
mendeley-tags = {B0320+39,B0809+74},
pages = {961--966},
primaryClass = {astro-ph},
title = {{The frequency-dependence of drifting subpulse patterns}},
