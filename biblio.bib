journal = {Astronomy and Astrophysics},
keywords = {Polarisation,drift,fourier,subpulse},
mendeley-tags = {Polarisation},
number = {2},
pages = {733--748},
primaryClass = {astro-ph},
title = {{Drifting sub-pulse analysis using the two-dimensional Fourier transform}},
url = {http://adsabs.harvard.edu/cgi-bin/nph-bib{\_}query?bibcode=2002A{\&}A...393..733E{\&}db{\_}key=AST},
volume = {393},
