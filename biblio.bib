@article{McKinney2006,
abstract = {The luminosity and structure of neutron star magnetospheres are crucial to our understanding of pulsar and plerion emission. A solution found using the force-free approximation would be an interesting standard with which any model with more physics could be compared. Prior quasi-analytic force-free solutions may not be stable, while prior time-dependent magnetohydrodynamic models used unphysical model parameters. We use a time-dependent relativistic force-free electrodynamics code with no free parameters to find a unique stationary solution for the axisymmetric rotating pulsar magnetosphere in a Minkowski space-time in the case of no surface currents on the star. The solution is similar to the force-free quasi-analytic solution of $\backslash$citet{\{}cont99{\}} and the numerical magnetohydrodynamic solution of $\backslash$citet{\{}kom05{\}}. The magnetosphere structure and the usefulness of the classical y-point in the general dissipative regime are discussed. The pulsar luminosity is found to be {\$}L \backslashapprox 0.99\backslashpm 0.01 \backslashmu{\^{}}2\backslashOmega{\_}\backslashstar{\^{}}4/c{\^{}}3{\$} for a dipole moment {\$}\backslashmu{\$} and stellar angular frequency {\$}\backslashOmega{\_}\backslashstar{\$}.},
archivePrefix = {arXiv},
arxivId = {astro-ph/0601411},
author = {McKinney, Jonathan C.},
doi = {10.1111/j.1745-3933.2006.00150.x},
