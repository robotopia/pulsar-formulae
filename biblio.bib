issn = {0004-637X, 1538-4357},
journal = {The Astrophysical Journal},
keywords = {47 Tuc},
mendeley-tags = {47 Tuc},
month = {may},
number = {1},
pages = {587--594},
title = {{Variability of Nineteen Millisecond Pulsars in 47 Tucanae with CHANDRA/HRC-S}},
