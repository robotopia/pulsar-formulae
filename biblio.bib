issn = {0004-637X},
journal = {The Astrophysical Journal},
number = {1},
pages = {270--285},
primaryClass = {arXiv:astro-ph},
title = {{The Characteristics of Millisecond Pulsar Emission. I. Spectra, Pulse Shapes, and the Beaming Fraction}},
