issn = {00358711},
journal = {Monthly Notices of the Royal Astronomical Society},
keywords = {Chaos, Methods: data analysis, Stars: kinematics a},
number = {2},
pages = {983--998},
title = {{Evidence for chaotic behaviour in pulsar spin-down rates}},
volume = {428},
year = {2013}
}
