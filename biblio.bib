   year = {2004},
}

@article{Mirza2017,
abstract = {Magnetic field plays a key role in the structure and evolution of neutron stars (NSs). While the primary source of magnetic field generation is generally believed to be the stellar core, we show that at least a part of NS magnetic field is due to spacetime frame-dragging in stellar vicinity. Solving the general relativistic Maxwell equations in slow rotating Kerr spacetime, we show that stellar rotation can induce mixed poloidal-toroidal magnetic field configuration in which toroidal field strength is comparable with the poloidal magnetic field, verifying the existence of strong toroidal magnetic fields observed in magnetars.},
