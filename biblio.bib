mendeley-tags = {Astrophysics - High Energy Astrophysical Phenomena},
month = {sep},
title = {{Emission from Pair-Instability Supernovae with Rotation}},
url = {http://arxiv.org/abs/1410.0039 http://www.arxiv.org/pdf/1410.0039.pdf},
year = {2014}
