mendeley-tags = {B0943+10,Polarisation},
number = {3},
pages = {1736--1745},
title = {{The topology and polarization of subbeams associated with the 'drifting' subpulse emission of pulsar B0943+10 - VI. Analysis of an 8-h Giant Metrewave Radio Telescope observation}},
url = {http://doi.wiley.com/10.1111/j.1365-2966.2011.19595.x},
