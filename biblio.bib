journal = {The Astrophysical Journal},
keywords = {Polarisation,aberration,beam,circular,model,morpho,polarization,pulseshape,rotation,rvm},
mendeley-tags = {Polarisation},
pages = {157},
primaryClass = {arXiv:submit},
title = {{Relativistic Model on Pulsar Radio Emission and Polarization}},
